.well
  %h3 Поиск
  #search_form
    = simple_form_for @search, :url => admin_linkers_path do |form|
      = form.input :id_eq, :required => false
      = form.input :url_like, :required => false
      = form.input :seo_text_like, :required => false
      = form.button :submit, 'Найти', class: 'btn'
